"#version 300 es\n"
"out vec4 fragColor;\n"
"\n"
"in vec3 ourColor;\n"
"in vec2 texCord;\n"
"\n"
"uniform sampler2D ourTexture1;\n"
"uniform sampler2D ourTexture2;\n"
"\n"
"void main()\n"
"{\n"
"    fragColor = mix(texture(ourTexture1, texCord), texture(ourTexture2, texCord), 0.2);\n"
"}\n"